{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultDuotone": false,
			"defaultPalette": false,
			"defaultGradients": false,
			"palette": [
				{
					"color": "#222",
					"name": "AB body",
					"slug": "ab-body"
				},
				{
					"color": "#FFF",
					"name": "AB body light",
					"slug": "ab-body-light"
				},
				{
					"color": "#333",
					"name": "AB heading",
					"slug": "ab-heading"
				},
				{
					"color": "#EF3444",
					"name": "AB red",
					"slug": "ab-red"
				},
				{
					"color": "#DEEDFB",
					"name": "AB light blue",
					"slug": "ab-light-blue"
				}
			]
		},
		"layout": {
			"contentSize": "800px",
			"wideSize": "1200px"
		},
		"spacing": {
			"customSpacingSize": false,
			"spacingSizes": [
				{
					"name": "1",
					"size": "1rem",
					"slug": "10"
				},
				{
					"name": "2",
					"size": "min(1.5rem, 2vw)",
					"slug": "20"
				},
				{
					"name": "3",
					"size": "min(2.5rem, 3vw)",
					"slug": "30"
				},
				{
					"name": "4",
					"size": "min(4rem, 5vw)",
					"slug": "40"
				},
				{
					"name": "5",
					"size": "min(6.5rem, 8vw)",
					"slug": "50"
				},
				{
					"name": "6",
					"size": "min(10.5rem, 13vw)",
					"slug": "60"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, avenir next, avenir, segoe ui, helvetica neue, helvetica, Cantarell, Ubuntu, roboto, noto, arial, sans-serif",
					"name": "System Sans-serif",
					"slug": "system-sans-serif"
				},
				{
					"fontFamily": "Iowan Old Style, Apple Garamond, Baskerville, Times New Roman, Droid Serif, Times, Source Serif Pro, serif, Apple Color Emoji, Segoe UI Emoji, Segoe UI Symbol",
					"name": "System Serif",
					"slug": "system-Serif"
				},
				{
					"fontFamily": "Lato",
					"slug": "lato",
					"fontFace": [
						{
							"fontFamily": "Lato",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/Lato/lato_normal_400.ttf"
							]
						},
						{
							"fontFamily": "Lato",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/Lato/lato_normal_700.ttf"
							]
						}
					]
				},
				{
					"fontFamily": "TeXGyreAdventor",
					"slug": "texgyreadventor",
					"fontFace": [
						{
							"fontFamily": "TeXGyreAdventor",
							"fontWeight": "700",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/TexGyreAdventor/texgyreadventor_normal_700.ttf"
							]
						}
					]
				}
			],
			"fontSizes": [
				{
                    "name": "Small",
					"slug": "small",
					"size": "0.9rem",
					"fluid": {
                        "min": "0.9rem",
                        "max": "1.rem"
                    }
				},
				{
                    "name": "Medium",
					"slug": "medium",
					"size": "1rem",
					"fluid": {
                        "min": "1rem",
                        "max": "1.2rem"
                    }
				},
                {
                    "name": "Large",
                    "slug": "large",
                    "size": "1.2rem",
                    "fluid": {
                        "min": "1.2rem",
                        "max": "1.44rem"
                    }
                },
                {
                    "name": "X Large",
                    "slug": "x-large",
                    "size": "1.44rem",
                    "fluid": {
                        "min": "1.44rem",
                        "max": "1.728rem"
                    }
                },
                {
                    "name": "2X Large",
                    "slug": "xx-large",
                    "size": "1.728rem",
                    "fluid": {
                        "min": "1.728rem",
                        "max": "2.074rem"
                    }
                },
                {
                    "name": "3X Large",
                    "slug": "xxx-large",
                    "size": "2.074rem",
                    "fluid": {
                        "min": "2.074rem",
                        "max": "2.488rem"
                    }
                },
                {
                    "name": "4X Large",
                    "slug": "xxxx-large",
                    "size": "2.488rem",
                    "fluid": {
                        "min": "2.488rem",
                        "max": "2.986rem"
                    }
                }
			],
			"writingMode": true
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"elements": {
			"button": {
				":active": {
					"color": {
						"background": "var(--wp--preset--color--ab-red)",
						"text": "var(--wp--preset--color--ab-body-light)"
					}
				},
				":focus": {
					"color": {
						"background": "var(--wp--preset--color--ab-red)",
						"text": "var(--wp--preset--color--ab-body-light)"
					},
					"border": {
						"color": "var(--wp--preset--color--ab-red)"
					}
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--ab-red)",
						"text": "var(--wp--preset--color--ab-body-light)"
					},
					"border": {
						"color": "var(--wp--preset--color--ab-red)"
					}
				},
				"border": {
					"radius": ".33rem",
					"color": "var(--wp--preset--color--ab-red)"
				},
				"color": {
					"background": "var(--wp--preset--color--ab-red)",
					"text": "var(--wp--preset--color--ab-body-light)"
				},
				"spacing": {
					"padding": {
						"bottom": "0.6rem",
						"left": "1rem",
						"right": "1rem",
						"top": "0.6rem"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)",
					"fontStyle": "normal",
					"fontWeight": "700",
					"fontFamily": "var(--wp--preset--font-family--heading)"
				}
			},
			"heading": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading)",
					"fontWeight": "400",
					"lineHeight": "1.2"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xxxx-large)",
					"lineHeight": "2"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xxx-large)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"link": {
				":hover": {
					"typography": {
						"textDecoration": "none"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				}
			}
		},
		"blocks": {
		},
		"spacing": {
			"blockGap": "1.2rem",
			"padding": {
				"top": "var(--wp--preset--spacing--50)",
				"left": "var(--wp--preset--spacing--50)",
				"bottom": "var(--wp--preset--spacing--50)",
				"right": "var(--wp--preset--spacing--50)"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--body)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"fontStyle": "normal",
			"fontWeight": "400",
			"lineHeight": "1.6"
		}
	}
}